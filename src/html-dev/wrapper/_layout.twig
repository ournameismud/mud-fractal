  </head>

  <body>
		<pre id="debug"></pre>
		<div class="flex">
			<header class="w-4/12">
				<ul class="list-reset p-1">
					{% for entry in menu %}
						<li class="text-lg mb-1">
							<a href="{{ entry.slug }}">{{ entry.title }}</a>
						</li>
					{% endfor %}
				</ul>
			</header>
			<main id="page-wrapper" class="w-full p-1">
				<div class="page-child">
					{% block content %}
					{% endblock %}
				</div>
			</main>
		</div>
    <script src="/dist/js/vendor.bundle{{ stamp }}.js"></script>
    <script async defer src="/dist/js/app.bundle{{ stamp }}.js"></script>
  </body>
</html>

