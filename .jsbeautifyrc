{
	"html": {
		"allowed_file_extensions": ["htm", "html", "xhtml", "shtml", "xml", "svg", "njk", "twig"],
		"brace_style": "collapse",
		"end_with_newline": false,
		"indent_char": " ",
		"indent_handlebars": false,
		"indent_inner_html": false,
		"indent_scripts": "keep",
		"indent_size": 2,
		"max_preserve_newlines": 0,
		"preserve_newlines": true,
		"unformatted": ["a", "span", "img", "code", "pre", "sub", "sup", "em", "strong", "b", "i", "u", "strike", "big",
			"small", "pre", "h1", "h2", "h3", "h4", "h5", "h6"
		],
		"wrap_line_length": 120
	},
	"css": {
		"allowed_file_extensions": ["css", "scss", "sass", "less"],
		"end_with_newline": true,
		"brace_style": "expanded",
		"indent_char": " ",
		"indent_size": 1,
		"indent_with_tabs": true,
		"max_preserve_newlines": 2,
		"newline_between_rules": true,
		"selector_separator_newline": true,
		"preserve_newlines": true,
		"keep_array_indentation": true,
    "keep_function_indentation": true,
    "space_before_conditional": true
	},
	"js": {
		"allowed_file_extensions": ["js", "jsx", "json", "eslintrc", "jsbeautifyrc"],
		"brace_style": "collapse",
		"break_chained_methods": false,
		"comma_first": false,
		"e4x": true,
		"end_with_newline": false,
		"indent_char": "  ",
		"indent_level": 0,
		"indent_size": 1,
		"jslint_happy": true,
		"indent_with_tabs": true,
		"keep_array_indentation": false,
		"keep_function_indentation": true,
		"max_preserve_newlines": 0,
		"preserve_newlines": true,
		"space_after_anon_function": false,
		"space_before_conditional": false,
		"space_in_empty_paren": false,
		"space_in_paren": false,
		"unescape_strings": false,
		"wrap_line_length": 120
	}
}
